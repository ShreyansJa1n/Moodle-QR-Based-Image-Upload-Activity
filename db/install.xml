<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/qrbasedimage/db" VERSION="20210812" COMMENT="XMLDB file for Moodle local/qrbasedimage"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_qrbasedimage" COMMENT="Default comment for local_qrbasedimage, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="randomnumber" COMMENT="randomnumber table storing random number generated by the user">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="9" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="ran_num" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="attemptid" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="tstamp" TYPE="datetime" NOTNULL="true" DEFAULT="CURRENT_TIMESTAMP" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="pk" TYPE="primary" FIELDS="id" COMMENT="pk"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="images" COMMENT="images table storing image link and relevant information">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="9" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="80" NOTNULL="true" DEFAULT="image" SEQUENCE="false"/>
        <FIELD NAME="image" TYPE="char" LENGTH="80" NOTNULL="true" DEFAULT="image" SEQUENCE="false"/>
        <FIELD NAME="ran_num" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="attemptid" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="uniqueid" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="tstamp" TYPE="datetime" NOTNULL="true" DEFAULT="CURRENT_TIMESTAMP" SEQUENCE="false"/>
        <FIELD NAME="quesid" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="slot" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="pk" TYPE="primary" FIELDS="id" COMMENT="pk"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
